{
  "name": "@nxpkmn/randoms",
  "version": "0.6.1",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "description": "Random team generation logic for Pok√©mon Showdown's Random Battle formats",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xerxicodex/ps.git"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build",
    "!build/test",
    "src",
    "!src/test"
  ],
  "dependencies": {
    "@nxpkmn/sim": "https://gitpkg.now.sh/xerxicodex/ps/sim"
  },
  "scripts": {
    "lint": "eslint --cache src --ext ts",
    "fix": "eslint --fix src --ext ts",
    "compile": "tsc -p .",
    "build": "npm run compile",
    "test": "jest",
    "pretest": "npm run build",
    "posttest": "npm run lint"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "node_modules/",
      "build/"
    ]
  },
  "eslintConfig": {
    "extends": "@pkmn",
    "rules": {
      "@typescript-eslint/quotes": "off",
      "@typescript-eslint/indent": [
        "error",
        "tab",
        {
          "flatTernaryExpressions": true
        }
      ],
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/prefer-optional-chain": "off",
      "@typescript-eslint/restrict-template-expressions": "off",
      "@typescript-eslint/member-delimiter-style": "off",
      "operator-linebreak": "off",
      "no-dupe-else-if": "off",
      "no-mixed-spaces-and-tabs": "off",
      "max-len": "off"
    }
  },
  "bugs": {
    "url": "https://github.com/xerxicodex/ps/issues"
  },
  "homepage": "https://github.com/xerxicodex/ps#readme",
  "devDependencies": {},
  "author": ""
}
